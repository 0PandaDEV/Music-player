{
  "productName": "Vleer",
  "version": "0.1.1",
  "identifier": "app.vleer",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm nuxt dev",
    "beforeBuildCommand": "pnpm nuxt generate"
  },
  "app": {
    "windows": [
      {
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "fullscreen": false,
        "height": 880,
        "resizable": true,
        "title": "Vleer",
        "width": 1580,
        "minHeight": 600,
        "minWidth": 800,
        "decorations": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEUwRUMwRTVDODZENTA2NjEKUldSaEJ0V0dYQTdzNE5lVjNsV0VmQUtNajl2Z2hCcTA4cGdURTI1MFVadENka3V1SG1OQkhER2cK",
      "endpoints": [
        "https://updater.vleer.app"
      ]
    },
    "sql": {
      "preload": [
        "sqlite:data.db"
      ]
    }
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}