{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "window:allow-create",
    "window:allow-maximize",
    "window:allow-toggle-maximize",
    "window:allow-unmaximize",
    "window:allow-minimize",
    "window:allow-unminimize",
    "window:allow-show",
    "window:allow-hide",
    "window:allow-close",
    "window:allow-set-decorations",
    "window:allow-start-dragging",
    "os:allow-platform",
    "dialog:allow-open",
    {
      "identifier": "fs:allow-audio-read-recursive",
      "allow": [
        {
          "path": "$AUDIO"
        },
        {
          "path": "$AUDIO/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-audio-write-recursive",
      "allow": [
        {
          "path": "$AUDIO"
        },
        {
          "path": "$AUDIO/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-audio-meta-recursive",
      "allow": [
        {
          "path": "$AUDIO"
        },
        {
          "path": "$AUDIO/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-appconfig-read-recursive",
      "allow": [
        {
          "path": "$APPCONFIG"
        },
        {
          "path": "$APPCONFIG/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-appconfig-write-recursive",
      "allow": [
        {
          "path": "$APPCONFIG"
        },
        {
          "path": "$APPCONFIG/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-appconfig-meta-recursive",
      "allow": [
        {
          "path": "$APPCONFIG"
        },
        {
          "path": "$APPCONFIG/**"
        }
      ]
    },
    "fs:default",
    "global-shortcut:allow-is-registered",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ]
}